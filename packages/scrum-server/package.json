  "main": "index.js",
  "scripts": {
    "build": "npm run clean && babel ./src --out-dir build --copy-files",
    "clean": "rm -rf build",
    "dev": "nodemon --exec babel-node ./src/bin/www",
    "lint": "eslint \"./**/*.js\" ",
    "lint-fix": "eslint --fix \"./**/*.js\"",
    "localStart": "node ./src/bin/www",
    "profile": "node --inspect ./src/bin/www",
    "start": "node ./build/bin/www",
    "test": "mocha --require @babel/register tests/*.js --exit",
    "testcoverage": "nyc mocha --require @babel/register tests/*.js --exit"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 52,
    "statements": 53
  },
